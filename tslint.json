{
  "extends": "tslint:recommended",
  "rules": {
    "arrow-parens": false,
    "ordered-imports": false,
    "curly": [
      true,
      "ignore-same-line"
    ],
    "no-trailing-whitespace": [
      true,
      "ignore-template-strings"
    ],
    "member-ordering": [
      true,
      "public-before-private",
      "static-before-instance",
      "variables-before-functions"
    ],
    "max-line-length": [
      true,
      100
    ],
    "quotemark": [
      true,
      "single"
    ]
  }
}
